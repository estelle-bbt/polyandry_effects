targets::tar_load(data_proxies)
data_proxy_av <- data_proxies$data_proxy_av
data_proxy_av <- data_proxy_av %>%
mutate(poll_treat_factor=fct_relevel(poll_treat_factor, c("low","medium","high")))
library(tidyverse)
data_proxy_av <- data_proxy_av %>%
mutate(poll_treat_factor=fct_relevel(poll_treat_factor, c("low","medium","high")))
# to apply likelihood ratio test, models must applied to the same data set -> always remove row with NA
mod_gMS_av_0 <-  glmer(data=(data_proxy_av %>% filter(!is.na(gMS))),cbind(nbGr_sum,nbAv_sum)~gMS*poll_treat_factor+(1|session),family=binomial)
mod_gMS_av_1 <-  glmer(data=(data_proxy_av %>% filter(!is.na(gMS))),cbind(nbGr_sum,nbAv_sum)~gMS+poll_treat_factor+(1|session),family=binomial)
library(lme4)
data_proxy_av <- data_proxy_av %>%
mutate(poll_treat_factor=fct_relevel(poll_treat_factor, c("low","medium","high")))
# to apply likelihood ratio test, models must applied to the same data set -> always remove row with NA
mod_gMS_av_0 <-  glmer(data=(data_proxy_av %>% filter(!is.na(gMS))),cbind(nbGr_sum,nbAv_sum)~gMS*poll_treat_factor+(1|session),family=binomial)
mod_gMS_av_1 <-  glmer(data=(data_proxy_av %>% filter(!is.na(gMS))),cbind(nbGr_sum,nbAv_sum)~gMS+poll_treat_factor+(1|session),family=binomial)
mod_gMS_av_2 <-  glmer(data=(data_proxy_av %>% filter(!is.na(gMS))),cbind(nbGr_sum,nbAv_sum)~poll_treat_factor+(1|session),family=binomial)
mod_gMS_av_3 <-  glmer(data=(data_proxy_av %>% filter(!is.na(gMS))),cbind(nbGr_sum,nbAv_sum)~gMS+(1|session),family=binomial)
anova(mod_gMS_av_0,mod_gMS_av_1) # inter
anova(mod_gMS_av_1,mod_gMS_av_2) # MS
anova(mod_gMS_av_1,mod_gMS_av_3) # ttt
